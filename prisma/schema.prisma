
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model users{
  id       Int    @id  @default(autoincrement())
  name     String
  email    String @unique
  password String
  credentials credentials[]
}


model credentials {
  id        Int    @id  @default(autoincrement())
  url       String 
  name      String 
  login     String 
  password  String
  userId    Int
  users     users  @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}


